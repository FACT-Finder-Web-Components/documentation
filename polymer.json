{
  "entrypoint": "index.html",
  "shell": "src/documentation-app.html",
  "fragments": [
    "src/views/documentation-view.html",
    "src/views/download-view.html",
    "src/views/contacts-view.html",
    "src/views/home-view.html",
    "src/views/view-404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/slate-font-awesome/fonts/*.*"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
